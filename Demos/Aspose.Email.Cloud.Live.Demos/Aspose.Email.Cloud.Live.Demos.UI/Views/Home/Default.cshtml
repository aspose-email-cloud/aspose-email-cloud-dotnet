@model Aspose.Email.Cloud.Live.Demos.UI.Models.LandingPageModel

@{
    ViewBag.Title = "Default";
}

<div class="container-fluid header1 header2 minify-header appfamily-header">
    <div class="container">
        <div class="row">
            <div class="col-md-8">
                <h1>Perform wide range of email processing operations.</h1>
                <h4>Including conversion of Eml, Msg, MsgUnicode, Mhtml, Html, Tnef and Oft formats in the cloud.</h4>
            </div>
            <div class="col-md-4 tc">
                <img class="col-xs-12" src="../img/aspose_email-for-cloud.png" alt="Aspose.Email Cloud" width="320" />
            </div>
        </div>
    </div>
</div>
<div class="container-fluid productfamilypage bg-white pdb-5">
    <div class="row">
        <div class="container tc">
            <div class="clearfix">&nbsp;</div>
            <div class="clearfix">&nbsp;</div>
            <h3>Aspose.Email Cloud .NET APIs Includes</h3>
            <div class="clearfix">&nbsp;</div>
            <div class="clearfix">&nbsp;</div>

            <div class="col-lg-4 productfamily_box">
                <div class="productfamilytitle">
                    <div class="imgblock">
                        <img src="../img/aspose_conversion-app.png" alt="Aspose.Email Cloud .NET SDK" />
                    </div>
                    <a href="/email/conversion"><span class="spanclass">Aspose.Email @Model.Resources["AsposeProductFor"] Cloud<em>Conversion</em></span><em>Easily convert Emails into EML, MSG, TNEF and OFT formats.</em></a>
                </div>
            </div>

        </div>
    </div>
</div>
<script type="text/javascript">
    window.onload = function () {
        alert('Please set "Client Id" and "Client Secret" in web.config to run Aspose.Email Cloud .NET APIs');
    };
</script> 